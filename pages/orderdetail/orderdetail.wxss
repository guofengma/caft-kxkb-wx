@import "./../../wxParse/wxParse.wxss" ;
.bg-img{
  width: 100%;
  /* height: 320rpx; */
}
.bg-img image{
  width: 100%;
  /* height: 100%; */
}
.detail-list{
  margin-bottom: 10rpx;
  background: #fff;
}
.detail-list .rcode{
  margin-bottom: 20rpx;
  color: #666;
}
.title{
  padding: 16rpx 120rpx 16rpx 30rpx;
  white-space: 10rpx;
  font-size: 26rpx;
  line-height: 36rpx;
  color: #343434;
}
.price{
  font-size: 24rpx;
  color: #D91414;
  line-height: 35rpx;
  padding: 0 30rpx 22rpx 30rpx;
  
}
.price::before{
  content: 'Â¥';
  font-size: 20rpx;
}
.order-list{
  padding-bottom: 30rpx;
}
.order-title{
  font-size: 30rpx;
  color: #595959;
  height: 76rpx;
  line-height: 76rpx;
  border-bottom: 2rpx solid #BEBEBE;
}
.order-title::before{
  content: '';
  background: #E60012;
  padding: 0rpx 4rpx; 
  margin-left: 30rpx;
  margin-right: 20rpx;
}
.order-info{
  margin: 0 66rpx 0 56rpx;
  border-bottom: 2rpx solid #BEBEBE;
  display: flex;
  /* padding: 30rpx 0 26rpx 0; */
  min-height: 82rpx;
  align-items: center;
}
.order-info .name{
  flex: 0.8;
  font-size: 30rpx;
  color: #595959;
}
.order-info .con{
  flex: 2.2;
  font-size: 24rpx;
  color: #848383;
}
.shop-info{
  padding: 16rpx 80rpx 56rpx 54rpx;
  display: flex;
  justify-content: space-between;
  align-items:center;
}
.address-icon,.phone-icon{
  width: 54rpx;
  height: 54rpx;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  display: inline-block;
  background-position: center center;
}
.address-icon{
  width: 40rpx;
  
  background-image: url('data:image/png;base64,');
}
.phone-icon{
  background-image: url('data:image/png;base64,');
}
.shop-info .info{
  font-size: 24rpx;
  color: #757575;
  display: inline-block;
  padding-left: 24rpx;
  padding-right: 24rpx;
  flex: 1;
}
.shop-info .info text{
  display: block;
}
.shop-info .info .name{
  color: #474747;
  font-size: 26rpx;
  padding-bottom: 8rpx;
}
.order-btn{
  padding: 20rpx 0 58rpx 0;
  display: flex;
  justify-content: center;
}
.order-btn .btn{
  padding: 18rpx 42rpx 16rpx 42rpx;
  border-radius: 6rpx;
  font-size: 30rpx;
  color: #FFFFFF;
  background: #2495FF;
}
.order-btn .btn.cancle-btn{
  color: #6E6E6E;
  background: #D0D0D0;
  margin-right: 58rpx;
}
.shop-list{
  background: #fff;
}