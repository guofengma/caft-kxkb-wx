.bg-image{
  width: 100%;
  height: 376rpx;
  position: relative;
}
.bg{
  width: 100%;
  height: 376rpx;
}
.fiter{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.3);
}
.search-box-wrapper{
  position: absolute;
  left: 0;
  top: 60rpx;
  margin: 0 0 0 34rpx;
  display:flex;
  align-items: center;
}
.car-icon{
  width: 62rpx;
  height: 50rpx;
  margin: -2rpx 0 0 0;
  background: url('data:image/png;base64,');
}
.search-box{
    display: inline-flex;
    vertical-align: top;
    align-items: center;
    box-sizing: border-box;
    width: 440rpx;
    padding: 0 14rpx;
    height: 54rpx;
    background: rgba(243,243,243,0.9);
    border-radius: 28rpx;
    margin: 0 26rpx 0 62rpx;
}
.search-icon{
    width: 30rpx;
    height: 28rpx;
}
.box{
  flex: 1;
  color: #959595;
  font-size: 28rpx;
  line-height: 28rpx;
  margin: 0 10rpx;
}
.city-wrapper{
  display: inline-flex;
  align-items: center;
}
.arrow-icon{
  width: 22rpx;
  height: 12rpx;
  display: inline-block;
  margin-left: 8rpx;
  margin-top: -4rpx;
  vertical-align: middle;
}
.txt{
  color: #fff;
  font-size: 24rpx;
  display: inline-block;
  width: 72rpx;
  overflow: hidden;
  white-space: nowrap;
}
.info-wrapper{
  display: flex;
  width: 100%;
  background: #fff;
  box-shadow: 2rpx 0 6rpx rgba(0,0,0,0.2);
}
.info{
  flex: 1;
  padding: 20rpx 30rpx 16rpx 30rpx;
}
.title{
  font-size: 30rpx;
  color: #1B1B1B;
}
.info-txt{
  font-size: 24rpx;
  color: #666464;
  line-height: 30rpx;
  margin: 24rpx 0 26rpx 0;
}
.address{
  font-size: 20rpx;
  color: #757575;
}
.info-icon{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  width: 166rpx;
  border-left: 2rpx solid #D2D2D2;
  padding: 20rpx 0 16rpx 0;
}
.icon{
  font-size: 20rpx;
  color: #859AFF;
  line-height: 30rpx;
  text-align: center
}
.nav-icon,.phone-icon{
  width: 38rpx;
  height: 38rpx;
  margin: 0 auto;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  margin-bottom: 4rpx;
}
.nav-icon{
  background-image: url('data:image/png;base64,');
}
.phone-icon{
  background-image: url('data:image/png;base64,');
}


.tab-wrapper{
  margin-top: 10rpx;
  padding: 34rpx 30rpx 36rpx 30rpx;
  background: #fff;
  display: flex;
  justify-content:space-between;
  box-shadow:2rpx 0 6rpx rgba(0,0,0,0.2);
}
.tab-icon{
  width: 80rpx;
  height: 80rpx;
  display: block;
}
.tab-item{
  text-align:center;
}
.tab-item text{
  color: #373737;
  font-size: 20rpx;
}
.banner{
  margin: 30rpx 0 20rpx 0;
  height: 208rpx;
}
.banner .img{
  width: 100%;
  height: 100%;
}
.carwash-wrapper{
  margin-bottom: 22rpx;
}
.carwash-title{
  text-align: center;
  width: 100%;
  height: 54rpx;
  line-height: 54rpx;
  color: #DDB603;
  font-size: 32rpx;
  background: #FFFAE1;
}
.carwash-img{
  width: 100%;
  height: 150rpx;
}
.carwash-img .img{
  width: 100%;
  height: 100%;
}