.detail-list{
  margin-bottom: 18rpx;
  background: #fff;
}
.detail-list .title{
  padding: 16rpx 30rpx 16rpx 30rpx;
  white-space: 10rpx;
  font-size: 30rpx;
  line-height: 36rpx;
  color: #343434;
  border-bottom:1rpx solid #eee;

}
.detail-list .title2{
  font-size: 24rpx;
  padding: 16rpx 30rpx 16rpx 30rpx;
}
.detail-list .price{
  font-size: 24rpx;
  color: #D91414;
  line-height: 35rpx;
  padding: 0 30rpx 22rpx 30rpx;
  
}
.price::before{
  content: 'Â¥';
  font-size: 20rpx;
}
.detail-list .discount{
    height: 96rpx;
    line-height: 96rpx;
    padding:0 30rpx;
    box-sizing: border-box;
    position: relative;
    display: flex;
    align-items: center;
    border-top: 1rpx solid #BEBEBE;
}
.discount .dis-img{
  display: inline-block;
  width: 92rpx;
  height: 24rpx;
  flex: 0 0 92rpx;
  margin-right: 44rpx;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  background-image: url('data:image/png;base64,');
}
.discount .txt{
  font-size:24rpx;
  color: #646464;
  flex: 2;
  padding-right: 68rpx;
  overflow: hidden;
  display: inline-block;
}
.order-title{
  font-size: 30rpx;
  color: #595959;
  height: 76rpx;
  line-height: 76rpx;
  border-bottom: 1rpx solid #f2f2f2;
}
.order-title::before{
  content: '';
  background: #E60012;
  padding: 0rpx 4rpx; 
  margin-left: 30rpx;
  margin-right: 20rpx;
}
.tab-wrapper{
  padding: 46rpx 30rpx 60rpx 30rpx;
  background: #fff;
  display: flex;
  justify-content:space-between;
}
.tab-icon{
  width: 106rpx;
  height: 106rpx;
  display: block;
}
.tab-item{
  text-align:center;
}
.tab-item text{
  color: #343434;
  font-size: 26rpx;
}
.table-info{
  margin: 40rpx 30rpx;
  font-size: 24rpx;
  color: #525252;
  border: 2rpx solid #BFBFBF;
  border-bottom: none;
  border-right: none;
}
.table-info .tr{
  display: flex;
  border-bottom: 2rpx solid #BFBFBF;
}

.table-info .tr .td{
  display: inline-block;
  border-right: 2rpx solid #BFBFBF;
  padding: 28rpx;
  flex: 2.5
}
.table-info .tr .td.name{
  flex: 1;
  color: #7B7A7A;
}
.detail-info{
  /* margin:0rpx 30rpx 34rpx 30rpx; */
  margin: 0 ;
  margin-bottom: 34rpx;
}
.detail-info .list{
  margin:0rpx 30rpx 34rpx 30rpx;
}
.detail-info .list .l-title{
  background: #EBF3FF;
  font-size: 36rpx;
  color: #045FEE;
  text-align: center;
  height: 94rpx;
  line-height: 94rpx;
}
.detail-info .list .l-title .txt{
  display:inline-block;
  position: relative;
  line-height: 50rpx;
}
.detail-info .list .l-title .txt.txt-icon{
   border-bottom: 2rpx solid #045FEE;   
}
.detail-info .list .l-title .txt.txt-icon .icon-box{
  position: absolute;
  left: 50%;
  width: 76rpx;
  margin-left: -38rpx;
  height: 4rpx;
  bottom: -2rpx;
  background: #EBF3FF;
}
.detail-info .list .l-title .txt.txt-icon .icon{
  position: absolute;
  left: 28rpx;
  width: 20rpx;
  height: 18rpx;
  bottom: -16rpx;
  background-repeat: no-repeat;
  background-position: center center;
  background-size: 100% 100%;
  background-image: url('data:image/png;base64,');
}
.detail-info .list .img{
  width: 100%;
  min-height: 340rpx;
}
.detail-info{
  padding-bottom: 28rpx;
}
.buy-list{
  background: #fff;
}
.buy-info{
  padding: 28rpx;
}
.buy-info .item-box{
  padding: 0 30rpx 28rpx 30rpx;
  font-size: 24rpx;
  color: #7B7A7A;
  display: flex;
}
.buy-info .item-box .icon{
  flex: 0 0 26rpx;
  width: 26rpx;
  height: 22rpx;
  margin-right: 26rpx;
  display: inline-block;
  margin-top: 8rpx;
  background-position: center center;
  background-repeat: no-repeat;
  background-size: 100%;
  background-image: url('data:image/png;base64,');
}
.buy-info .item{
  flex:2;
}
.buy-info .item text{
  display: block;
  line-height: 36rpx;
}
.buy-info .item .name{
  color: #525252;
}
.detail-btn{
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 10;
  width: 100%;
  border-top: 2rpx solid #979797;
  box-sizing:border-box;
}
.detail-btn .btn{
  display: inline-block;
  width: 50%;
  background: #fff;
  font-size: 30rpx;
  text-align: center;
  height: 100rpx;
  line-height: 100rpx;
}
.detail-btn .btn::before{
  height: 100%;
  background-size: 100%;
  vertical-align: middle;
}
.detail-btn .btn.shop-icon::before,.detail-btn .btn.dis-icon::before{
  content: '';
  display: inline-block;
  background-position: center center;
  background-repeat: no-repeat;
  background-size: 100%;
  margin-right: 22rpx;
} 
.detail-btn .btn.shop-icon::before{
  width: 40rpx;
  background-image: url('data:image/png;base64,');
}
.detail-btn .btn.dis-icon::before{
  width: 46rpx;
  background-image: url('data:image/png;base64,');
}
.detail-btn .btn.dis-icon{
  background: #D91414;
  color: #fff; 
}